CREATE TABLE USSD.USSDTransaction (uuid VARCHAR(30) NOT NULL, aquirerId VARCHAR(30), bankCode VARCHAR(30), dateCreated TIMESTAMP, flowStatus VARCHAR(30), message VARCHAR(250), mno VARCHAR(30), sendSms SMALLINT, sessionId VARCHAR(30), sourceMobile VARCHAR(20), version INTEGER, PRIMARY KEY (uuid));
CREATE TABLE USSD.WebSession (id VARCHAR(30) NOT NULL, bankId VARCHAR(30), dateCreated TIMESTAMP, firstIndex INTEGER, message VARCHAR(250), mobileNumber VARCHAR(20), passwordRetryCount INTEGER, referenceId VARCHAR(30), secondIndex INTEGER, sendSms SMALLINT, status VARCHAR(20), timeout TIMESTAMP, version INTEGER, PRIMARY KEY (id));
