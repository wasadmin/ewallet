<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="BankInterfaceEJB">
		<jta-data-source>jdbc/EWALLET</jta-data-source>
		<class>zw.co.esolutions.bankif.model.TransactionInfo</class>
		<class>zw.co.esolutions.bankif.model.CommissionMessage</class>
		<class>zw.co.esolutions.bankif.model.BankResponseCode</class>
		<class>zw.co.esolutions.bankif.model.BankRequestMessage</class>
		<properties>
			<property name="openjpa.jdbc.Schema" value="BANKIF"/>
		</properties>
	</persistence-unit>
</persistence>
