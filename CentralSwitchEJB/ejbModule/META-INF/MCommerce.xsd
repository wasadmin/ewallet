	<complexType name="Transaction">
		<sequence>
			<element name="additionalData" type="string" /><!-- 
				yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="uuid" type="string" /><!-- Generated 
				by Payment Gateway -->
			<element name="airtimeBalance" type="double" /><!-- 
				Generated by VAS Gateway -->
			<element name="amount" type="double" />
			<element name="bankAccountBalance" type="double" />
			<element name="bankId" type="string" /><!-- beneficiary 
				subscribe -->
			<element name="currency" type="string" />
			<element name="narrative" type="string" />
			<element name="mobileNetworkOperator">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="ECONET"></enumeration>
    					<enumeration value="NETONE"></enumeration>
    					<enumeration value="TELECEL"></enumeration>
    				</restriction>
    			</simpleType>    		
    		</element>
			<element name="customerId" type="string" />
			<element name="passwordPrompt" type="string" />
			<element name="dateProcessed" type="dateTime" />
			<element name="dateReceived" type="dateTime" />
			<element name="mobileNumber" type="string" />
			<element name="referenceNo" type="string" />
			<element name="replyQueue" type="string" />
			<element name="responseCode" type="string" />
			<element name="serviceId" type="string" />
			<element name="sourceAccount" type="string" />
			<element name="status" type="string" />
			<element name="targetAccount" type="string" />
			<element name="targetMobile" type="string" />
			<element name="transactionType" type="string" />
			<element name="utility" type="string" />
			<element name="merchantReference" type="string" />
		</sequence>
	</complexType>

	<complexType name="SettlementReportRes">												<!-- Sent by VAS Gateway to Payment Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="uuid" type="string" />   					<!-- Generated by Payment Gateway -->
			<element name="dateRequested" type="string" /> 	<!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" /> 		<!-- beneficiary subscribe -->
			<element name="sourceId" type="string" />
			<element name="serviceCommand" type="string" /> 	<!-- bill payment -->
			<element name="amount" type="double" />					<!-- For checking account balance, min, max values -->
			<element name="issuingBankBin" type="string" />
			<element name="issuingBankName" type="string" />
			<element name="bankReference" type="string" />
			<element name="status" type="string" />   				<!-- ACK, NACK -->
			<element name="narration" type="string" />
			<element name="accountBalance" type="double" />
			<element name="expiryDate" type="string" />
		</sequence>
	</complexType>

	<complexType name="Error">
		<sequence>
			<element name="version" type="string" />
			<element name="errorCode" type="string" />
			<element name="errorMessage" type="string" />
			<element name="errorDetail" type="string" />
			<element name="vendorCode" type="string"
				minOccurs="0" maxOccurs="1" />
		</sequence>
	</complexType>
