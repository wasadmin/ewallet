   	<element name="BankRegistrationMessage" type="tns:BankRegistrationMessage"></element>
   	
	<complexType name="Message">
		<sequence>
			<choice>
				<element name="MobileOriginatedShortMessage" type="tns:MobileOriginatedShortMessage" />
				<element name="MobileOriginatedShortMessageRes" type="tns:MobileOriginatedShortMessageRes" />
				<element name="MobileTerminatedShortMessage" type="tns:MobileTerminatedShortMessage" />
				<element name="MobileTerminatedShortMessageRes" type="tns:MobileTerminatedShortMessageRes" />
				<element name="DeliveryReport" type="tns:DeliveryReport" />
				<element name="DeliveryReportRes" type="tns:DeliveryReportRes" />
				<element name="MobileSubscriberQuery" type="tns:MobileSubscriberQuery" />
				<element name="MobileSubscriberQueryRes" type="tns:MobileSubscriberQueryRes" />
				<element name="TopupReq" type="tns:TopupReq" />
				<element name="TopupRes" type="tns:TopupRes" />
