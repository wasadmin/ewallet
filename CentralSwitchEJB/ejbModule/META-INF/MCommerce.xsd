			<element name="dateDelivered" type="string" /> <!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" />
			<element name="status" type="string" />   			<!-- Refer to SMPP3.4 specification, section 5.1.3 -->
			<element name="narration" type="string" />			<!-- Refer to SMPP3.4 specification, section 5.1.3 -->
		</sequence>
	</complexType>

	<complexType name="DeliveryReportRes">												<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="userMessageId" type="string" />
			<element name="uuid" type="string" />
			<element name="smscMessageId" type="string" />
			<element name="uuid" type="string" />
			<element name="status" type="string" />
			<element name="narration" type="string" />
		</sequence>
	</complexType>

	<complexType name="MobileSubscriberQuery">											<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="uuid" type="string" />   					<!-- Generated by Payment Gateway -->
			<element name="sourceMobile" type="string" />
			<element name="targetMobile" type="string" />
			<element name="dateRequested" type="dateTime" /> 	<!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="bankServiceId" type="string" />
			<element name="serviceCommand" type="string" /> 	<!-- topup, bill payment -->
			<element name="amount" type="double" />					<!-- For checking account balance, min, max values -->
		</sequence>
	</complexType>

	<complexType name="MobileSubscriberQueryRes">										<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="uuid" type="string" />   					<!-- Generated by Payment Gateway -->
			<element name="dateRequested" type="dateTime" /> 	<!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="dateProcessed" type="dateTime" /> 	<!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" /> 		<!-- beneficiary subscribe -->
			<element name="currentBalance" type="double" />
			<element name="serviceClass" type="string" />
			<element name="subscriberType" type="string" />
			<element name="creditLimit" type="double" />
			<element name="minimumTopup" type="double" />
			<element name="maximumTopup" type="double" />
			<element name="expiryDate" type="dateTime" />     	<!-- yyyy-MM-dd -->
			<element name="responseCode" type="string" />   				<!-- ACK, NACK -->
			<element name="narrative" type="string" />
			<element name="vasReference" type="string" />
		</sequence>
	</complexType>

