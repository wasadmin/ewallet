<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://msg.mcommerce.esolutions.co.zw" xmlns:tns="http://msg.mcommerce.esolutions.co.zw" elementFormDefault="qualified">

   <complexType name="MobileProfile">
    	<sequence>
    		<element name="mobileNetworkOperator">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="ECONET"></enumeration>
    					<enumeration value="NETONE"></enumeration>
    					<enumeration value="TELECEL"></enumeration>
    				</restriction>
    			</simpleType>
    		</element>
    		<element name="mobileNumber" type="string"></element>
    		<element name="secretCode" type="string"></element>
    		<element name="passwordRetryCount" type="int"></element>
    		<element name="timeout" type="dateTime"></element>
    		<element name="customerId" type="string"></element>
    		<element name="referralCode" type="string"></element>
    		<element name="referralProcessed" type="boolean"></element>
    		<element name="dateCreated" type="dateTime"></element>
    		<element name="status">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="ACTIVE"></enumeration>
    					<enumeration value="DISAPPROVED"></enumeration>
    					<enumeration value="INACTIVE"></enumeration>
    					<enumeration value="AWAITING_APPROVAL"></enumeration>
    					<enumeration value="LOCKED"></enumeration>
    					<enumeration value="DELETED"></enumeration>
    					<enumeration value="HOT"></enumeration>
    				</restriction>
    			</simpleType>
    		</element>
    		<element name="id" type="string"></element>
    		<element name="bankId" type="string"></element>
    		<element name="primary" type="boolean"></element>
    		<element name="notification" type="string"></element>
    	</sequence>
   </complexType>
    
   
    <complexType name="ContactDetails">
    	<sequence>
    		<element name="contactName" type="string"></element>
    		<element name="id" type="string"></element>
    		<element name="telephone" type="string"></element>
    		<element name="email" type="string"></element>
    		<element name="fax" type="string"></element>
    		<element name="street" type="string"></element>
    		<element name="surburb" type="string"></element>
    		<element name="city" type="string"></element>
    		<element name="country" type="string"></element>
    		<element name="ownerId" type="string"></element>
    		<element name="ownerType" type="string"></element>
    		<element name="dateCreated" type="dateTime"></element>
    	</sequence>
    </complexType>

    <complexType name="BankBranch">
    	<sequence>
    		<element name="branchName" type="string"></element>
    		<element name="branchCode" type="string"></element>
    		<element name="id" type="string"></element>
    		<element name="contactDetailsId" type="string"></element>
    		<element name="dateCreated" type="dateTime"></element>
    		<element name="bankId" type="string"></element>
    		<element name="status">
    		   <simpleType>
    		   	<restriction base="string">
    		   		<enumeration value="ACTIVE"></enumeration>
    		   		<enumeration value="DISABLED"></enumeration>
    		   		<enumeration value="AWAITING_APPROVAL"></enumeration>
    		   		<enumeration value="INACTIVE"></enumeration>
    		   		<enumeration value="DISAPPROVED"></enumeration>
    		   		<enumeration value="DELETED"></enumeration>
    		   	</restriction>
    		   </simpleType>
    		</element>
    	</sequence>
    </complexType>

    <complexType name="Bank">
    	<sequence>
    		<element name="bankName" type="string"></element>
    		<element name="code" type="string"></element>
    		<element name="contactDetailsId" type="string"></element>
    		<element name="dateCreated" type="dateTime"></element>
    		<element name="id" type="string"></element>
    		<element name="status">
    		   <simpleType>
    		   	<restriction base="string">
    		   		<enumeration value="ACTIVE"></enumeration>
    		   		<enumeration value="DISABLED"></enumeration>
    		   		<enumeration value="AWAITING_APPROVAL"></enumeration>
    		   		<enumeration value="INACTIVE"></enumeration>
    		   		<enumeration value="DISAPPROVED"></enumeration>
    		   		<enumeration value="DELETED"></enumeration>
    		   	</restriction>
    		   </simpleType>
    		</element>
    	</sequence>
    </complexType>

    <complexType name="Customer">
    	<sequence>
    		<element name="title" type="string"></element>
    		<element name="lastName" type="string"></element>
    		<element name="firstNames" type="string"></element>
    		<element name="nationalId" type="string"></element>
    		<element name="contactDetailsId" type="string"></element>
    		<element name="dateOfBirth" type="dateTime"></element>
    		<element name="gender">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="MALE"></enumeration>
    					<enumeration value="FEMALE"></enumeration>
    					<enumeration value="UNSPECIFIED"></enumeration>
    				</restriction>
    			</simpleType>
    		</element>
    		<element name="maritalStatus">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="SINGLE"></enumeration>
    					<enumeration value="MARRIED"></enumeration>
    					<enumeration value="WIDOWED"></enumeration>
    					<enumeration value="UNSPECIFIED"></enumeration>
    				</restriction>
    			</simpleType>
    		</element>
    		<element name="branchId" type="string"></element>
    		<element name="dateCreated" type="dateTime"></element>
    		<element name="status">
    		   <simpleType>
    		   	<restriction base="string">
    		   		<enumeration value="DRAFT"></enumeration>
    		   		<enumeration value="AWAITING_ACTIVATION"></enumeration>
    		   		<enumeration value="ACTIVE"></enumeration>
    		   		<enumeration value="SUSPENDED"></enumeration>
    		   		<enumeration value="TERMINATED"></enumeration>
    		   		<enumeration value="DELETED"></enumeration>
    		   		<enumeration value="AWAITING_APPROVAL"></enumeration>
    		   		<enumeration value="INACTIVE"></enumeration>
    		   		<enumeration value="DISAPPROVED"></enumeration>
    		   	</restriction>
    		   </simpleType>
    		</element>
    		<element name="customerClass">
    			<simpleType>
    				<restriction base="string">
    					<enumeration value="REGULAR"></enumeration>
    					<enumeration value="EMPLOYEE"></enumeration>
    					<enumeration value="AGENT"></enumeration>
    				</restriction>
    			</simpleType>
    		</element>
    		<element name="id" type="string"></element>
    		<element name="customerAutoRegStatus" type="string"></element>
    	</sequence>
    </complexType>

 	<complexType name="MobileOriginatedShortMessage">							<!-- Sent by VAS Gateway to Payment Gateway -->
		<sequence>
			<element name="version" type="string" />
			<element name="smscMessageId" type="string" />
			<element name="uuid" type="string" /><!-- Generated by VAS Gateway -->
			<element name="dateReceived" type="dateTime" /><!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" />
			<element name="destinationId" type="string" />
			<element name="shortMessage" type="string" />
			<element name="mobileNetworkOperator">
				<simpleType>
					<restriction base="string">
						<enumeration value="NETONE"></enumeration>
						<enumeration value="ECONET"></enumeration>
						<enumeration value="TELECEL"></enumeration>
					</restriction>
				</simpleType>
			</element>
			<element name="originatorId">
				<simpleType>
					<restriction base="string">
						<enumeration value="ECONET_SMPP"></enumeration>
						<enumeration value="ECONET_USSD"></enumeration>
						
					</restriction>
				</simpleType>
			</element>
		</sequence>
 	</complexType>

	<complexType name="MobileOriginatedShortMessageRes">					<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="smscMessageId" type="string" />
			<element name="uuid" type="string" />
			<element name="status" type="string" />
			<element name="narration" type="string" />
		</sequence>
	</complexType>

	<complexType name="MobileTerminatedShortMessage">							<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="userMessageId" type="string" />	<!-- Generated by Payment Gateway -->
			<element name="uuid" type="string" />   				<!-- Generated by Payment Gateway -->
			<element name="dateRequested" type="string" /> <!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" />
			<element name="shortMessage" type="string" />
			<element name="deliveryReportRequired" type="boolean" />
		</sequence>
	</complexType>

	<complexType name="MobileTerminatedShortMessageRes">					<!-- Sent by Payment Gateway to VAS Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="userMessageId" type="string" />	<!-- Generated by Payment Gateway -->
			<element name="uuid" type="string" />   				<!-- Generated by Payment Gateway -->
			<element name="smscMessageId" type="string" />	<!-- Generated by SMSC -->
			<element name="dateRequested" type="string" /> <!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="dateSent" type="string" /> 			<!-- yyyy-MM-dd HH:mm:ss.SSS -->
			<element name="subscriberId" type="string" />
			<element name="status" type="string" />   			<!-- Refer to SMPP3.4 specification, section 5.1.3 -->
			<element name="narration" type="string" />			<!-- Refer to SMPP3.4 specification, section 5.1.3 -->
		</sequence>
	</complexType>

	<complexType name="DeliveryReport">														<!-- Sent by VAS Gateway to Payment Gateway -->

		<sequence>
			<element name="version" type="string" />
			<element name="userMessageId" type="string" />
			<element name="uuid" type="string" />   				<!-- Generated by Payment Gateway -->
			<element name="smscMessageId" type="string" />	<!-- Generated by SMSC -->
			<element name="dateRequested" type="string" /> <!-- yyyy-MM-dd HH:mm:ss.SSS -->
