	<navigation-rule>
		<from-view-id>/login/login.jspx</from-view-id>
		<navigation-case>
			<from-action>#{loginBean.login}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/reportsweb/reportsHome.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{loginBean.clear}</from-action>
			<from-outcome>clear</from-outcome>
			<to-view-id>/login/login.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login/errorPage.jspx</from-view-id>
		<navigation-case>
			<from-action>#{errorPageBean.back}</from-action>
			<from-outcome>back</from-outcome>
			<to-view-id>/reportsweb/reportsHome.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/login/logout.jspx</from-view-id>
		<navigation-case>
			<from-action>#{loginBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login/login.jspx</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{loginBean.cancelLogout}</from-action>
			<from-outcome>cancelLogout</from-outcome>
			<to-view-id>/reportsweb/reportsHome.jspx</to-view-id>
		</navigation-case>
	</navigation-rule>
